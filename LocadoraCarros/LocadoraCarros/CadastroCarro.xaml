<Window x:Class="LocadoraCarros.CadastroCarro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocadoraCarros"
        mc:Ignorable="d"
        Title="CadastroCarro" Height="351.828" Width="522.258">
    <Grid Margin="-16,-2,16,2">
        <Label x:Name="label" Content="Carro" HorizontalAlignment="Left" Margin="88,34,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label x:Name="label_Copy" Content="Modelo" HorizontalAlignment="Left" Margin="78,60,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy1" Content="Ano" HorizontalAlignment="Left" Margin="97,88,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy2" Content="Placa" HorizontalAlignment="Left" Margin="92,116,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy3" Content="Quilometragem" HorizontalAlignment="Left" Margin="41,140,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy4" Content="Cor" HorizontalAlignment="Left" Margin="100,171,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy5" Content="Chaci" HorizontalAlignment="Left" Margin="90,198,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy6" Content="nº Portas:" HorizontalAlignment="Left" Margin="68,226,0,0" VerticalAlignment="Top"/>
