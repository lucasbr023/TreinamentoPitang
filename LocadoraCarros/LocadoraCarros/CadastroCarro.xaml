        <TextBox x:Name="textBox_Ano" HorizontalAlignment="Left" Height="23" Margin="134,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353" TextChanged="textBox_Ano_TextChanged"/>
        <TextBox x:Name="textBox_Placa" HorizontalAlignment="Left" Height="23" Margin="134,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353" TextChanged="textBox_Placa_TextChanged"/>
