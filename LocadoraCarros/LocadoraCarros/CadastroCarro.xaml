        <TextBox x:Name="textBox_Cor" HorizontalAlignment="Left" Height="23" Margin="134,170,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353" TextChanged="textBox_Cor_TextChanged"/>
        <TextBox x:Name="textBox_Chaci" HorizontalAlignment="Left" Height="23" Margin="134,198,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353" TextChanged="textBox_Chaci_TextChanged"/>
        <TextBox x:Name="textBox_qtdPortas" HorizontalAlignment="Left" Height="23" Margin="134,226,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353" TextChanged="textBox_qtdPortas_TextChanged"/>
        <Label x:Name="label_Copy7" Content="Categoria:" HorizontalAlignment="Left" Margin="65,252,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="134,254,0,0" VerticalAlignment="Top" Width="138" SelectionChanged="comboBox_SelectionChanged_1"/>
