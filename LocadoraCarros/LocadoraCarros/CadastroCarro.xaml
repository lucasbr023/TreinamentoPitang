<Window x:Class="LocadoraCarros.CadastroCarro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocadoraCarros"
        mc:Ignorable="d"
        Title="CadastroCarro" Height="350" Width="525">
    <Grid>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="41,31,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label x:Name="label_Copy" Content="Modelo" HorizontalAlignment="Left" Margin="41,57,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy1" Content="Ano" HorizontalAlignment="Left" Margin="41,83,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy2" Content="Placa" HorizontalAlignment="Left" Margin="41,109,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy3" Content="Quilometragem" HorizontalAlignment="Left" Margin="41,140,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy4" Content="Cor" HorizontalAlignment="Left" Margin="41,166,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy5" Content="Chaci" HorizontalAlignment="Left" Margin="42,192,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy6" Content="Quantidade de Portas" HorizontalAlignment="Left" Margin="42,218,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Modelo" HorizontalAlignment="Left" Height="23" Margin="97,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="390"/>
        <TextBox x:Name="textBox_Ano" HorizontalAlignment="Left" Height="23" Margin="97,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="390"/>
        <TextBox x:Name="textBox_Placa" HorizontalAlignment="Left" Height="23" Margin="97,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="390"/>
        <TextBox x:Name="textBox_Quilometragem" HorizontalAlignment="Left" Height="23" Margin="134,143,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="353"/>
        <TextBox x:Name="textBox_Cor" HorizontalAlignment="Left" Height="23" Margin="97,170,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="390"/>
        <TextBox x:Name="textBox_Chaci" HorizontalAlignment="Left" Height="23" Margin="97,198,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="390"/>
        <TextBox x:Name="textBox_qtdPortas" HorizontalAlignment="Left" Height="23" Margin="167,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="320"/>
        <Label x:Name="label_Copy7" Content="Categoria:" HorizontalAlignment="Left" Margin="42,244,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="111,248,0,0" VerticalAlignment="Top" Width="138"/>
        <Button x:Name="button_salvarCarro" Content="Salvar" HorizontalAlignment="Left" Margin="412,289,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
